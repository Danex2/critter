{
  "name": "findmypet",
  "version": "1.0.0",
  "engines": {
    "node": "13.7.0"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "watch": "cross-env NODE_ENV=development nodemon --exec babel-node index.js",
    "build": "babel . --ignore node_modules,.babelrc,package-lock.json,package.json,tests,frontend,jest.config.js -d dist",
    "prod": "npm run build && npm run start",
    "start": "node dist/index.js",
    "docker": "cross-env NODE_ENV=docker nodemon --exec babel-node --require index.js",
    "run:test": "jest --verbose test",
    "run:cov": "jest --coverage",
    "heroku-postbuild": "npm run build && cd client && npm install && npm run build"
  },
  "dependencies": {
    "babel-plugin-dotenv": "^0.1.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.16.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.7",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^2.2.1",
    "node-geocoder": "^3.25.0",
    "path": "^0.12.7",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@types/jest": "^24.0.22",
    "core-js": "^3.3.6",
    "cross-env": "^6.0.3",
    "jest": "^24.9.0",
    "mongodb-memory-server": "^6.0.1",
    "nodemon": "^1.19.4",
    "regenerator-runtime": "^0.13.3",
    "supertest": "^4.0.2"
  }
}
